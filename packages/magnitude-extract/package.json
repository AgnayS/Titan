{
  "name": "magnitude-extract",
  "version": "0.0.2",
  "description": "TypeScript DOM cleaning and structuring library",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "unstructured-ts": "./dist/cli.js"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "bun test",
    "lint": "bun run eslint src/**/*.ts",
    "prepublishOnly": "bun run build"
  },
  "keywords": [
    "dom",
    "html",
    "cleaning",
    "parsing",
    "structured-data"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "cheerio": "^1.2.0",
    "domhandler": "^5.0.3",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@types/bun": "^1.3.6",
    "@types/cheerio": "^1.0.0",
    "@types/node": "^25.0.10",
    "@types/uuid": "^11.0.0",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "eslint": "^9.39.2",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3"
  }
}